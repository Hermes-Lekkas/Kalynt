{
    "name": "kalynt",
    "version": "1.0.5-beta",
    "private": true,
    "description": "Serverless P2P Team Collaboration Platform",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "npm run dev --workspace=apps/desktop",
        "dev:web": "npm run dev:web --workspace=apps/desktop",
        "electron:dev": "npm run electron:dev --workspace=apps/desktop",
        "electron:dev:linux": "npm run electron:dev:linux --workspace=apps/desktop",
        "build": "npm run build --workspace=apps/desktop",
        "build:secure": "npm run build:secure --workspace=apps/desktop",
        "electron:build:secure": "npm run electron:build:secure --workspace=apps/desktop",
        "electron:build": "npm run electron:build --workspace=apps/desktop",
        "test": "npm run test --workspaces --if-present",
        "lint": "eslint . --ext .ts,.tsx"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@typescript-eslint/eslint-plugin": "^8.53.0",
        "@typescript-eslint/parser": "^8.53.0",
        "electron": "^40.6.1",
        "eslint": "^8.57.1",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-security": "^2.1.1",
        "typescript": "5.7.3"
    },
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.0.0"
    },
    "overrides": {
        "@babel/runtime": ">=7.26.10",
        "tar": ">=7.5.8",
        "minimist": ">=1.2.8",
        "node-fetch": ">=2.7.0",
        "semver": ">=7.7.1",
        "class-validator": ">=0.14.1",
        "validator": ">=13.15.0",
        "rollup": ">=4.58.0",
        "ajv": ">=6.14.0",
        "tmp": ">=0.2.3"
    },
    "author": {
        "name": "Hermes Lekkas",
        "email": "hermeslekkasdev@gmail.com"
    },
    "license": "AGPL-3.0-only"
}
