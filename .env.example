# Kalynt Environment Variables Template
# Copy this file to .env and fill in your values

# ==========================================
# API Endpoints (optional - defaults provided)
# ==========================================
# VITE_OPENAI_API_URL=https://api.openai.com/v1
# VITE_ANTHROPIC_API_URL=https://api.anthropic.com/v1
# VITE_GOOGLE_API_URL=https://generativelanguage.googleapis.com/v1beta
# VITE_GITHUB_API_URL=https://api.github.com

# ==========================================
# Feature Flags
# ==========================================
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_TELEMETRY=false

# ==========================================
# Development
# ==========================================
# VITE_DEV_SERVER_URL=http://localhost:5173
# OPEN_DEVTOOLS=false

# ==========================================
# Model Configuration
# ==========================================
# KALYNT_MODELS_DIR=/path/to/custom/models/directory
# VITE_MODEL_REGISTRY_URL=/models.json
# VITE_MODEL_UPDATE_URL=https://api.kalynt.com/models/registry

# ==========================================
# Security & Performance
# ==========================================
# VITE_MAX_FILE_SIZE_MB=50
# VITE_ENCRYPTION_PBKDF2_ITERATIONS=100000
# VITE_AI_REQUEST_TIMEOUT_MS=30000

# ==========================================
# Free Beta Settings
# ==========================================
# No limits during free beta!
# Unlimited workspaces, collaborators, and AI models
